@page
@model HospiEnCasa.App.Presentacion.Pages_BDMedicos.DetailsModel
@{
}

<h1>Detalles</h1>
<div>
    <h4>Medico</h4>
    <dl class="row">
        <dt class="col-sm-2">Nombre</dt>
        <dd class="col-sm-10">@Model.medico.Nombre</dd>
        <dt class="col-sm-2">Apellidos</dt>
        <dd class="col-sm-10">@Model.medico.Apellido</dd>
        <dt class="col-sm-2">Documento</dt>
        <dd class="col-sm-10">@Model.medico.Documento</dd>
        <dt class="col-sm-2">Tarjeta Profesional</dt>
        <dd class="col-sm-10">@Model.medico.TarjetaProfesional</dd>
        <dt class="col-sm-2">Genero</dt>
        <dd class="col-sm-10">@Model.medico.GeneroId</dd>
        <dt class="col-sm-2">Telefono</dt>
        <dd class="col-sm-10">@Model.medico.Telefono</dd>
        <dt class="col-sm-2">Registro Rethus</dt>
        <dd class="col-sm-10">@Model.medico.Rethus</dd>
        <dt class="col-sm-2">Especialidad</dt>
        <dd class="col-sm-10">@Model.medico.Especialidad</dd>
    </dl>
