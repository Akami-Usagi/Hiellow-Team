@page
@model HogarGestor.App.Presentacion.Pages_BDFamiliares.DeleteModel
@{
}
<h1>Seguro que desea eliminar el siguiente Familiar?</h1>
<div>
    <h4>Joven</h4>
    <dl class="row">
        <dt class="col-sm-2">Id</dt>
        <dd class="col-sm-10">@Model.familiar.Id</dd>
        <dt class="col-sm-2">Nombre</dt>
        <dd class="col-sm-10">@Model.familiar.Nombre</dd>
        <dt class="col-sm-2">Apellidos</dt>
        <dd class="col-sm-10">@Model.familiar.Apellidos</dd>
        <dt class="col-sm-2">Documento</dt>
        <dd class="col-sm-10">@Model.familiar.Documento</dd>
        <dt class="col-sm-2">Parentesco</dt>
        <dd class="col-sm-10">@Model.familiar.Parentesco</dd>
        <dt class="col-sm-2">Genero</dt>
        <dd class="col-sm-10">@Model.familiar.Genero</dd>
        <dt class="col-sm-2">Telefono</dt>
        <dd class="col-sm-10">@Model.familiar.NumeroTelefono</dd>
        <dt class="col-sm-2">Correo Electronico</dt>
        <dd class="col-sm-10">@Model.familiar.CorreoElectronico</dd>
    </dl>
</div>

<form method="post">
    <input type="hidden" asp-for="familiar.Id">
    <button type="submit" class="btn btn-primary" asp-page-handler="Delete">Eliminar Familiar</button>
    <a asp-page="./index">Back to List</a>
</form>